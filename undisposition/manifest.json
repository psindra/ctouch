{
	"author": "--",
	"name": "Undisposition",
	"description": "Remove Content-Disposition: attachment HTTP header.",
	"version": "0.1",
	"manifest_version": 2,
	"homepage_url": "https://github.com/cielavenir/ctouch/undisposition",
	"background": {
		"scripts": ["undisposition_bg.js"],
		"persistent": true
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	]
}
